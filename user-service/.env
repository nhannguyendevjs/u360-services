# Host env
HOST_ENV = localhost
HOSTNAME = localhost
HOST_PORT = 8080

# App
APP_DOMAIN = u360
APP_VERSION = 1.0.0
API_VERSION = v1
APP_ENV = dev

# Request
MAX_REQUEST_BODY_SIZE = 50mb

# Logger
ENABLE_LOGGER_CONSOLE = true
# ENABLE_LOGGER_FILE = true

# MongoDB
ENABLE_MONGO = true
MONGO_URI = mongodb://admin:admin@localhost:27017/

# Redis
ENABLE_REDIS = true
REDIS_HOST = localhost
REDIS_PORT = 6379

# BullMQ
ENABLE_BULLMQ = true
BULLMQ_ADMIN_PATH = /admin/queues

# JWT
JWT_ALGORITHM = HS256
JWT_ACCESS_TOKEN_SECRET = 
JWT_REFRESH_TOKEN_SECRET = 
JWT_ACCESS_TOKEN_EXPIRATION_TIME = 1d
JWT_REFRESH_TOKEN_EXPIRATION_TIME = 365d
